{
  "name": "phrjss",
  "version": "12.0.0",
  "description": "Application utilizing Sitecore JavaScript Services and Angular (angular-cli).",
  "config": {
    "appName": "phrjss",
    "rootPlaceholders": [
      "jss-header",
      "jss-main",
      "jss-footer"
    ],
    "sitecoreDistPath": "/dist/phrjss",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "buildArtifactsPath": "./dist",
    "graphQLEndpointPath": "/api/phrjss",
    "language": "en"
  },
  "engines": {
    "node": ">=8.1",
    "npm": ">=5.6.0",
    "yarn": "yarn is not supported, please use npm"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "jss": "jss",
    "start": "npm-run-all --serial bootstrap:disconnected --parallel start:angular start:proxy start:watch-components",
    "start:connected": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:angular start:watch-components",
    "build:prod": "npm-run-all --serial bootstrap:connected --parallel build:client:prod build:server:prod --serial build:server-wrapper",
    "build:staging": "npm-run-all --serial bootstrap:connected --parallel build:client:staging build:server:staging --serial build:server-wrapper",
    "build": "npm-run-all --serial bootstrap:connected --parallel build:client build:server --serial build:server-wrapper",
    "scaffold": "ng generate @sitecore-jss/sitecore-jss-angular-schematics:jss-component",
    "start:angular": "ng serve -o --base-href=/",
    "start:proxy": "ts-node scripts/disconnected-mode-proxy.ts",
    "start:watch-components": "ts-node scripts/generate-component-factory.ts --watch",
    "build:client": "cross-env-shell ng build --configuration=dev --deploy-url $npm_package_config_sitecoreDistPath/browser/ --output-path=$npm_package_config_buildArtifactsPath/browser/",
    "build:client:staging": "cross-env-shell ng build --configuration=staging --deploy-url $npm_package_config_sitecoreDistPath/browser/ --output-path=$npm_package_config_buildArtifactsPath/browser/",
    "build:client:prod": "cross-env-shell ng build --configuration=production --deploy-url $npm_package_config_sitecoreDistPath/browser/ --output-path=$npm_package_config_buildArtifactsPath/browser/",
    "build:server": "cross-env-shell ng run phrjss:server --output-path=$npm_package_config_buildArtifactsPath/server --output-hashing=none",
    "build:server:staging": "cross-env-shell ng run phrjss:server:stage --output-path=$npm_package_config_buildArtifactsPath/server --output-hashing=none",
    "build:server:prod": "cross-env-shell ng run phrjss:server:prod --output-path=$npm_package_config_buildArtifactsPath/server --output-hashing=none",
    "build:server-wrapper": "cross-env-shell webpack --config webpack.server.config.js --progress --output-path $npm_package_config_buildArtifactsPath",
    "bootstrap:connected": "ts-node scripts/bootstrap.ts",
    "bootstrap:disconnected": "ts-node scripts/bootstrap.ts --disconnected",
    "graphql:update": "ts-node ./scripts/update-graphql-fragment-data.ts",
    "api:phrwebapi": "node_modules/.bin/ng-openapi-gen --config phr-swagger/phr-webapi-config.json"
  },
  "private": true,
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "homepage": "https://jss.sitecore.net",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "7.3.7",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-server": "8.2.14",
    "@angular/router": "8.2.14",
    "@material/select": "6.0.0",
    "@ng-bootstrap/ng-bootstrap": "5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "7.0.2",
    "@ngx-meta/core": "7.0.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@nicky-lenaers/ngx-scroll-to": "3.0.1",
    "@sitecore-jss/sitecore-jss": "12.0.0",
    "@sitecore-jss/sitecore-jss-angular": "12.0.2",
    "@sitecore-jss/sitecore-jss-tracking": "12.0.0",
    "@types/jquery": "3.3.31",
    "angular-oauth2-oidc": "8.0.4",
    "apollo-angular": "1.8.0",
    "apollo-angular-link-http-batch": "1.9.0",
    "apollo-angular-link-persisted": "1.1.0",
    "apollo-cache-inmemory": "1.3.12",
    "apollo-client": "2.4.13",
    "apollo-link": "1.2.6",
    "body-scroll-lock": "^4.0.0-beta.0",
    "bootstrap": "4.2.1",
    "core-js": "2.6.2",
    "crypto-js": "4.1.1",
    "font-awesome": "4.7.0",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "hammerjs": "2.0.8",
    "lodash": "4.17.20",
    "ng-recaptcha": "5.0.0",
    "ng2-pdf-viewer": "5.2.4",
    "ngx-dropzone": "2.5.0",
    "ngx-gallery": "5.10.0",
    "ngx-print": "1.2.0-beta.5",
    "ngx-toastr": "10.2.0",
    "ngx-ui-loader": "7.2.2",
    "node-sass": "4.12.0",
    "rxjs": "6.6.7",
    "tslib": "1.9.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.29",
    "@angular/cli": "8.3.29",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@sitecore-jss/sitecore-jss-angular-schematics": "12.0.0",
    "@sitecore-jss/sitecore-jss-cli": "12.0.0",
    "@sitecore-jss/sitecore-jss-dev-tools": "12.0.0",
    "@sitecore-jss/sitecore-jss-manifest": "12.0.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.18",
    "body-parser": "1.18.3",
    "chalk": "2.4.2",
    "codelyzer": "5.2.0",
    "cookie-parser": "1.4.3",
    "cross-env": "5.2.0",
    "enhanced-resolve": "3.3.0",
    "express": "4.16.4",
    "fs-extra": "7.0.1",
    "isomorphic-fetch": "2.2.1",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-openapi-gen": "0.11.1",
    "ng-swagger-gen": "1.8.1",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.1",
    "ts-loader": "5.2.2",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.5.1",
    "webpack-cli": "3.3.12"
  }
}
